<div class="page" id="page">
	{{> organisms-header }}
	<div role="main">
		<div class="row">
			<div class="large-12 columns">
				<h1 class="page__title">{{ page-title }}</h1>
			</div>
			<div class="pricingBody">
				{{# pricing}}
					<div class="large-6 columns">
						{{> molecules-pricing-table }}
					</div>
				{{/ pricing}}
				{{# trail }}
					<div class="trail">
						<div class="large-5 columns">
							<h3 class="trial__text"> {{ trail-text }} </h3>
						</div>
						<div class="large-7 columns">
							<a href="" class="large round button trail__button">{{ trail-button }}</a>
						</div>
					</div>
				{{/ trail}}
				{{# business-plan}}
					<div class="large 12 columns">
						<div class="businessPlan">{{ business }}</div>
					</div>
				{{/ business-plan}}
			</div>
		</div>

	</div><!--End role=main-->
	{{> organisms-footer }}
</div>