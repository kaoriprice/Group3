<div class="page" id="page">
	{{> organisms-header }}
	<div role="main">
		<div class="row">
			<div class="large-12 columns">
				<h1 class="page-title">{{ page-title }}</h1>
			</div>
			<div class="staffBody">
				{{# pricing}}
					<div class="large-6 columns">
						{{> molecules-pricing-table }}
					</div>
				{{/ pricing}}
				{{# trail }}
					<div class="large-4 columns">
						<h3 class="trial__text"> {{ trail-text }} </h3>
					</div>
					<div class="large-8 columns">
						<a href="" class="large round button">{{ trail-button }}</a>
					</div>
				{{/ trail}}
				{{# business-plan}}
					<div class="large 12 columns">
						{{ business }}
					</div>
				{{/ business-plan}}
			</div>
		</div>

	</div><!--End role=main-->
	{{> organisms-footer }}
</div>