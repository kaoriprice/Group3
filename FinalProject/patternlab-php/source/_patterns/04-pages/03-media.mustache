<div class="page" id="page">
	{{> organisms-header }}
	<div role="main">
		<h1 class="page-title">{{ page-title }}</h1>
		<div class="g g-3up">
			{{img.square}}
			{{# media__item}}
				<section class="img">
					{{img.landscape-4x3}}
				</section>
			{{/ media__item}}
		</div><!--end 3up-->

	</div><!--End .main-->
	{{> organisms-footer }}
</div>


<div class="page" id="page">
	{{> organisms-header }}
	<div class="main">

		{{# hero }}
			{{> organisms-hero-intro }}
		{{/ hero}}

		<div class="g g-3up">
			{{# touts}}
				<div class="gi">
					{{> molecules-inset-block }}
				</div>
			{{/ touts}}
		</div><!--end 3up-->

		<hr />

		<div class="l-two-col">
			<div class="l-main">
				<section class="section latest-posts">
					<h2 class="section-title">Latest Posts</h2>
					<ul class="post-list">
						{{# latest-posts}}
							<li>{{> molecules-media-block }}</li>
						{{/ latest-posts}}
					</ul>
					<a href="#" class="text-btn">View more posts</a>
				</section>
			</div>

			<div class="l-sidebar">
				{{> organisms-recent-tweets }}
			</div><!--end l-sidebar-->
	</div><!--End .main-->
	{{> organisms-footer }}
</div>